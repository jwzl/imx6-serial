+static void hdmi_enable_audio_clk(hdmi_data_info_s * hdmi, int enable)
+{
+	u8 clkdis;
+
+	clkdis = readb(HDMI_MC_CLKDIS);
+	if (enable)
+		clkdis &= ~HDMI_MC_CLKDIS_AUDCLK_DISABLE;
+	else
+		clkdis |= HDMI_MC_CLKDIS_AUDCLK_DISABLE;
+	writeb(clkdis, HDMI_MC_CLKDIS);
+}
