+ * mute hdmi audio output
+ */
+static void hdmi_audio_mute(hdmi_data_info_s * hdmi, int mute)
+{
+	writebf((mute == TRUE) ? 0xF : 0, HDMI_FC_AUDSCONF, 4, 4);
